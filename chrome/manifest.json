{
    "manifest_version": 3,
    "name": "Geoguessr Stats",
    "version": "0.0.5",
    "description": "Collect data of your GeoGuessr party games.",
    "key": "",
    "action": {
        "default_title": "Geoguessr Stats"
    },
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "world": "MAIN",
            "matches": [
                "https://www.geoguessr.com/*"
            ],
            "js": [
                "content_main.js"
            ],
            "run_at": "document_start"
        },
        {
            "world": "ISOLATED",
            "matches": [
                "https://www.geoguessr.com/*"
            ],
            "js": [
                "content_isolated.js"
            ],
            "run_at": "document_start"
        }
    ]
}